{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "bpw-wcm-service": [
        "dist/bpw-wcm-service"
      ],
      "bpw-wcm-service/*": [
        "dist/bpw-wcm-service/*"
      ],
      "bpw-wcm-preview": [
        "dist/bpw-wcm-preview"
      ],
      "bpw-wcm-preview/*": [
        "dist/bpw-wcm-preview/*"
      ],
      "bpw-wcm-authoring": [
        "dist/bpw-wcm-authoring"
      ],
      "bpw-wcm-authoring/*": [
        "dist/bpw-wcm-authoring/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}