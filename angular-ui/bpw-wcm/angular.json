            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/bpm-ui/e2e/protractor.conf.js",
            "devServerTarget": "bpm-ui:serve"
