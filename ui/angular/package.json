    "@angular/platform-server": "^10.2.2",
    "@angular/router": "~10.0.8",
    "@ngneat/until-destroy": "^7.1.5",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nguniversal/builders": "^10.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@ngx-translate/core": "13.0.0",
    "@swimlane/dragula": "^3.8.0",
    "@swimlane/ngx-charts": "^14.0.0",
    "@swimlane/ngx-datatable": "^17.1.0",
    "@swimlane/ngx-dnd": "^8.1.2",
    "@types/express": "^4.17.8",
    "@types/prismjs": "^1.16.1",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "ajv": "^6.12.2",
    "angular-calendar": "^0.28.16",
    "angular-in-memory-web-api": "^0.11.0",
    "bpmn-js": "^7.3.0",
    "bpmn-js-in-color": "github:bpmn-io/bpmn-js-in-color",
    "bpmn-js-properties-panel": "^0.35.0",
    "camunda-bpmn-moddle": "^4.4.0",
    "camunda-cmmn-moddle": "^1.0.0",
    "camunda-dmn-moddle": "^1.0.0",
    "chart.js": "^2.9.3",
    "classlist.js": "^1.1.20150312",
    "cmmn-js": "^0.20.0",
    "cmmn-js-properties-panel": "^0.8.3",
    "d3": "^5.16.0",
    "date-fns": "^2.15.0",
    "dmn-js": "^9.2.1",
    "dmn-js-properties-panel": "^0.5.0",
    "document-register-element": "^1.14.3",
    "express": "^4.17.1",
    "google-libphonenumber": "^3.2.14",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "net": "^1.0.2",
    "ng2-charts": "^2.3.2",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-build-plus": "^9.0.6",
    "ngx-color-picker": "^9.1.0",
    "ngx-cookie-service": "^3.1.2",
    "ngx-perfect-scrollbar": "^9.0.0",
    "perfect-scrollbar": "^1.5.0",
    "prismjs": "^1.20.0",
    "rxjs": "~6.6.0",
    "sockjs-client": "^1.4.0",
    "stompjs": "^2.3.3",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.8",
    "@angular/language-service": "~10.0.8",
    "@angularclass/hmr": "^2.1.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@ngrx/schematics": "^8.6.0",
    "@semantic-release/exec": "^5.0.0",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "4.14.149",
    "@types/node": "^13.7.1",
    "chai": "^4.2.0",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.0.2",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "projects/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
