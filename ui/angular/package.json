{
  "name": "bpw-wcm",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-wcm-authoring:dev:ssr": "ng run wcm-authoring:serve-ssr",
    "dev:ssr": "ng run wcm-authoring:serve-ssr",
    "serve:ssr": "node dist/wcm-authoring/server/main.js",
    "build:ssr": "ng build --prod && ng run wcm-authoring:server:production",
    "prerender": "ng run wcm-authoring:prerender",
    "start-bpmn-modeler": "ng serve --port 7009 --host=localhost --project bpmn-modeler",
    "start-bpw-form": "ng serve --port 4090 --host=localhost --project bpw-form --vendorSourceMap --proxy-config proxy.conf.json",
    "start-wcm-authoring": "ng serve --port 3009 --host=wcm-authoring --project wcm-authoring --proxy-config proxy.conf-mysite.json",
    "start-bpm-ui": "ng serve --port 4009 --host=bpm-ui  --project bpm-ui",
    "start-mysite": "ng serve --port 6009 --host=mysite  --project mysite",
    "start-material-theme-flex-poc": "ng serve --port 3100 --project material-theme-flex-poc",
    "start-bpw-skeleton-minimal": "ng serve --port 3200 --project bpw-skeleton-minimal",
    "start-bpw-skeleton": "ng serve --port 3300 --project bpw-skeleton",
    "start-bpw-reference-app": "ng serve --port 3500 --project bpw-reference-app",
    "start-hmr-wcm-authoring": "ng serve --project wcm-authoring --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps-wcm-authoring": "ng serve --project wcm-authoring --configuration hmr --source-map=true --hmr-warning=false",
    "build-wcm-authoring": "ng build --dev --project wcm-authoring",
    "build-stats-wcm-authoring": "ng build --dev --project wcm-authoring --stats-json",
    "build-prod-wcm-authoring": "ng build --project wcm-authoring --prod --output-hashing none",
    "build-prod-aot-wcm-authoring": "ng build --project wcm-authoring --prod --aot --buildOptimizer --commonChunk --vendorChunk --optimization --progress --output-hashing none",
    "build-prod-stats-wcm-authoring": "ng --project wcm-authoring build --prod --stats-json",
    "start-hmr-bpm-ui": "ng serve --project bpm-ui --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps-bpm-ui": "ng serve --project bpm-ui --configuration hmr --source-map=true --hmr-warning=false",
    "build-bpm-ui": "ng build --dev --project bpm-ui",
    "build-stats-bpm-ui": "ng build --dev --project bpm-ui --stats-json",
    "build-prod-bpm-ui": "ng build --project bpm-ui --prod --output-hashing none",
    "build-prod-bpw-skeleton-minimal": "ng build --project bpw-skeleton-minimal --prod --output-hashing none",
    "build-prod-aot-bpm-ui": "ng build --project bpm-ui --prod --aot --buildOptimizer --commonChunk --vendorChunk --optimization --progress --output-hashing none",
    "build-prod-stats-bpm-ui": "ng --project bpm-ui build --prod --stats-json",
    "build-prod-aot-mysite": "ng build --project mysite --prod --aot --buildOptimizer --commonChunk --vendorChunk --optimization --progress --output-hashing none",
    "test-coverage": "ng test --code-coverage --watch=false",
    "upload-coverage": "TODO: found the jenkins plugin for this",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "format:check": "prettier --list-different 'projects/**/*.ts'",
    "format:write": "prettier --write 'projects/**/*.ts'",
    "build:showcase": "ng build authoring-ui --base-href='https://frankli999.github.io/authoring-ui/'",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/athoring-ui",
    "copy:readme": "copyfiles ./README.md ./dist/authoring-ui",
    "build:lib": "npm run copy:readme && ng build authoring-ui",
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/ng-simple-alert/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION",
    "copy-styles": "cpx \"./projects/bpw-common/src/lib/scss/**/*\" \"./dist/bpw-common/lib/scss\" && cpx \"./projects/bpw-common/src/lib/**/*.theme.scss\" \"./dist/bpw-common/lib\"",
    "copy-wcm-styles": "cpx \"./projects/bpw-wcm-authoring/src/lib/scss/**/*\" \"./dist/bpw-wcm-authoring/lib/scss\" && cpx \"./projects/bpw-wcm-authoring/src/lib/**/*.theme.scss\" \"./dist/bpw-wcm-authoring/lib\""
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~10.0.8",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.8",
    "@angular/compiler": "~10.0.8",
    "@angular/core": "~10.0.8",
    "@angular/elements": "^10.0.8",
