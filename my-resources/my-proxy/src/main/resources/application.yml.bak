server:
  port: 8760

eureka:
  client:
    serviceUrl:
      defaultZone: http://user:password@localhost:8761/eureka/

logging:
  pattern: 
    console: "%d{yyyy-MM-dd HH:mm:ss} ${LOG_LEVEL_PATTERN:-%5p} %m%n"
              
zuul:
  routes:
    resource:
      path: /resource/**
      serviceId: my-service